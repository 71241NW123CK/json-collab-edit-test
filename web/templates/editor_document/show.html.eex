<h2>Show editor document</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @editor_document.title %>
  </li>

  <li>
    <strong>Document:</strong>
    <%= @editor_document.document %>
  </li>

  <li>
    <strong>Owner:</strong>
    <%= @editor_document.owner_id %>
  </li>

</ul>

<div id="embed_elm_json_editor" userid="1" documentid="<%= @editor_document.id %>"></div>

<%= link "Edit", to: editor_document_path(@conn, :edit, @editor_document) %>
<%= link "Back", to: editor_document_path(@conn, :index) %>
